{
 "name": "goodreads-search-app",
 "version": "1.0.0",
 "description": "Goodreads Search App",
 "main": "src/index.html",
 "scripts": {
  "start-web": "better-npm-run start:web",
  "prebuild-dev": "npm run clean-dist & npm run compile",
  "build-dev": "better-npm-run build:dev",
  "clean-dist": "rimraf dist/** & rimraf build/**",
  "compile": "tsc -d"
 },
 "betterScripts": {
  "start:web": {
   "command": "node --max-old-space-size=2048 ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./webpack/dev.config.js --compress true --output-pathinfo --inline --hot",
   "env": {
    "NODE_ENV": "development",
    "startDevServer": true
   }
  },
  "build:dev": {
   "command": "node --max-old-space-size=2048 ./node_modules/webpack/bin/webpack --config ./webpack/dev.config.js --bail",
   "env": {
    "NODE_ENV": "development",
    "analyze": false
   }
  }
 },
